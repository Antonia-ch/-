/*--------------------------------------------------------------------------------------------------------------------*/


/*ÇóÈ«×é¸÷¿ÆµÄÆ½¾ù³É¼¨ºÍ¸÷¿Æ×ÜÆ½¾ù³É¼¨*/
void main(void)
{
	/*******************************************/
	/*ÒÔÏÂÎª³ÌÐò³õÊ¼»¯ÇøÓò£¬´ËÇøÓòµÄ³ÌÐòÔÚ³ÌÐòÕý³£ÔËÐÐÖÐÖ»»áÖ´ÐÐÒ»´Î*/
	/*******************************************/
	/*´úÂë±àÐ´Çø:¿ÉÒÔ¶¨Òå¾Ö²¿±äÁ¿µÈ */
	unsigned int score[5][3]={
		                          {80,75,92},
                              {61,65,71},
															{59,63,70},
															{85,87,90},
															{76,77,85}
                             }; 
    unsigned int v[3];
    unsigned int sum_score;
    unsigned int ave_score;
    unsigned int i;												 
    unsigned int j;
	/*´úÂë±àÐ´Çø:±äÁ¿³õÊ¼»¯ */
	 
     sum_score = 0;
     ave_score = 0;
	   i = 0;
		 j = 0;
	/*******************************************/
	/*ÒÔÉÏÎª³ÌÐò³õÊ¼»¯ÇøÓò£¬´ËÇøÓòµÄ³ÌÐòÔÚ³ÌÐòÕý³£ÔËÐÐÖÐÖ»»áÖ´ÐÐÒ»´Î*/
	/*******************************************/
loop:/*³ÌÐòÖ÷Ñ­»·*/
	   /*´úÂë±àÐ´Çø:¿ÉÒÔÐ´´úÂë */

														 
	for(i=0;i<3;i++)
 {
    for(j=0;j<5;j++)
	 {
		 sum_score = sum_score + score[j][i];
		 
	 }
	 v[i] =  sum_score/5;
	 sum_score = 0;
 }	
  
 ave_score = (v[0]+v[1]+v[2])/3;
 
	
	
	goto loop;/*³ÌÐòÔËÐÐµ½Õâ¸öµØ·½£¬ÎÞÌõ¼þÌø×ªµ½loop±êºÅËùÔÚµÄÐÐ*/
}



